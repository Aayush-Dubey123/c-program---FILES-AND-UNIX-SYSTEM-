// Online C compiler to run C program online
#include <stdio.h>
#include <stdlib.h>
int main ()
{
    FILE *fptr;
    fptr = fopen("new file","w");
    if(fptr == NULL)
    {
        printf("new file created");
        exit(0);
    }
    else
    {
        fprintf(fptr,"new file created");
    }
    return 0;
}
